#include <stdio.h>

int main() {
    int n, i;

    // Entrada de dados
    printf("Digite um número inteiro para ver sua tabuada: ");
    scanf("%d", &n);

    // Estrutura de repetição for
    printf("\nTabuada do %d:\n", n);
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", n, i, n * i);
    }

    return 0;
}
